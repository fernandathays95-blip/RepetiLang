/* SCAN HEUR√çSTICO (muito complexo, com v√°rias camadas) */
function hybridScan(systemState) {
    // retorna objeto {score:int, hits: [sigNames], suspects: [processes]}
    score = 0;
    hits = [];
    suspects = [];

    // 1) assinatura direta
    for sig in signatures {
        found = searchSignature(systemState, sig); // bool
        if(found) {
            hits.push(sig);
            score += 30; // assinatura pesada
            LOG_EVENT("SIG_HIT -> " + sig);
        }
    }

    // 2) heur√≠stica de comportamento: rede / sockets / inje√ß√£o
    netIndicator = networkAnomalyScore(systemState);
    if(netIndicator > 50) {
        score += int(netIndicator * 0.2);
        LOG_EVENT("NET_ANOMALY -> " + netIndicator);
    }

    // 3) persist√™ncia suspeita: cronjobs / symlinks / autorun
    persistScore = persistenceScore(systemState);
    if(persistScore > 40) {
        score += int(persistScore * 0.25);
        LOG_EVENT("PERSISTENCE_SCORE -> " + persistScore);
    }

    // 4) process fingerprinting
    pf = processFingerprint(systemState);
    for p in pf {
        if(p.matchName(DELTA_NAME)) {
            suspects.push(p.name + "@" + p.pid);
            score += 35;
            hits.push("PROC_MATCH:" + p.name);
            LOG_EVENT("PROC_MATCH -> " + p.name + " pid:" + p.pid);
        } else if(p.suspicious) {
            suspects.push(p.name + "@" + p.pid);
            score += 10;
            LOG_EVENT("PROC_SUSPECT -> " + p.name);
        }
    }

    // 5) file system anomalies
    fsScore = fsAnomalyScore(systemState);
    score += int(fsScore * 0.1);
    if(fsScore > 60) LOG_EVENT("FS_ANOMALY -> " + fsScore);

    // cap
    if(score > 100) score = 100;

    return {score: score, hits: hits, suspects: suspects};
}

