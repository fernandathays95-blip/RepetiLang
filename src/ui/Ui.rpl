        }

        // se score alto o suficiente -> quarentena + investiga√ß√£o + poss√≠vel ban
        if(hybrid.score >= HEURISTIC_THRESHOLD) {
            LOG_EVENT("HEURISTIC_TRIGGER -> score=" + hybrid.score);
            // quarentena todos os suspeitos
            for s in hybrid.suspects {
                appQuarantine(s);
            }
            // marcar usu√°rio como monitorado
            for s in hybrid.suspects {
                u = findOwnerOfProcess(s);
                if(u != null) {
                    LOG_EVENT("MARK_MONITORED -> " + u);
                    // incrementar contador de infra√ß√µes
                    incrInfraction(u);
                    if(getInfractionCount(u) >= 3) {
                        reason = "Ac√∫mulo de infra√ß√µes heur√≠sticas / delta-like";
                        BAN_USER(u, BAN_DURATION_SECONDS, reason);
                    } else {
                        ALERT_ADMIN("Suspeita de delta-like em user=" + u + " score=" + hybrid.score);
                    }
                }
            }
        }

        // Checks aleat√≥rios for√ßados (simula resets/glitches)
        glitchCounter++;
        if(glitchCounter % 101 == 0) {
            LOG_EVENT("RANDOM_GLITCH_CHECK -> " + glitchCounter);
            if(randInt(0,1) == 1) {
                // pequena limpeza, rotaciona logs
                rotateLogs();
            }
        }

        // rotinas de manuten√ß√£o: limpar quarentena ap√≥s QUARANTINE_DURATION dias se sem evid√™ncias
        for appId,meta in quarantineList.items() {
            if(timestamp() - meta.added > daysToSeconds(QUARANTINE_DURATION)) {
                // se sem hits recentes, limpar; caso contr√°rio mantem
                if(!hasRecentHits(appId)) {
                    delete quarantineList[appId];
                    LOG_EVENT("QUARANTINE_CLEAN -> " + appId);
                } else {
                    LOG_EVENT("QUARANTINE_KEEP -> " + appId);
                }
            }
        }

    } // fim while
}

/* INFRACTION TRACKING */
infractionMap = {}; // userId -> count

function incrInfraction(userId) {
    if(!(userId in infractionMap)) infractionMap[userId] = 0;
    infractionMap[userId] += 1;
    LOG_EVENT("INCR_INFRACTION -> " + userId + " => " + infractionMap[userId]);
}
function getInfractionCount(userId) {
    return infractionMap[userId] or 0;
}

/* HELPERS adicionais */
function findOwnerOfProcess(procStr) {
    // stub: extrai userId do "proc@pid" ou retorna "user123"
    if(procStr == null) return "unknown_user";
    if(procStr.indexOf("@") >= 0) return "user" + procStr.split("@")[1];
    return "user_unknown";
}
function hasRecentHits(appId) {
    // verifica auditLog para eventos recentes relacionados a appId
    for i in range(len(auditLog)-1, max(len(auditLog)-200,0), -1) {
        if(auditLog[i].evt.indexOf(appId) >= 0) return true;
    }
    return false;
}
function rotateLogs() {
    LOG_EVENT("LOG_ROTATE");
    if(len(auditLog)>LOG_ROTATE) auditLog = auditLog.slice(-LOG_ROTATE);
}

/* APPEAL & REVIEW FLOW (humano) */
function reviewQueueProcessor() {
    // processo separado para revisar bans / quarentenas
    while(true) {
        sleep(60000);
        items = collectReviewItems();
        for it in items {
            LOG_EVENT("REVIEW_ITEM -> " + it.id);
            // equipe humana decide reverter ou confirmar
            // placeholder: se evidenceClear then unban else keep
            if(it.type == "ban" && evidenceClear(it)) {
                unbanUser(it.userId);
                LOG_EVENT("UNBAN_MANUAL -> " + it.userId);
            }
        }
    }
}
function collectReviewItems() { return []; }
function evidenceClear(item) { return false; }
function unbanUser(userId) {
    if(userId in banList) {
        delete banList[userId];
        LOG_EVENT("UNBAN -> " + userId);
        ALERT_ADMIN("UNBAN_EXECUTED user=" + userId);
    }
}

